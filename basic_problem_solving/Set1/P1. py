# 1D array

a=[1,2,3,4,5]
print(a)
print(a[0])
print (type (a))
print (type (a[2]))

b=[[1,2],[3,4]]
print (b)
print (type(b))
print (b[1])
print (type(b[1]))
print (b[0][1])
print (type(b[0][1]))

A = [[2,4],[1,0]]
print(A)
print (A*3)
print(A[0][0]*3)
print(A[0][0]*3,A[0][1]*3,A[1][0]*3,A[1][1]*3)

n=[1,2,3,4,5]
for i in range (len(n)):
    print (n[i])
